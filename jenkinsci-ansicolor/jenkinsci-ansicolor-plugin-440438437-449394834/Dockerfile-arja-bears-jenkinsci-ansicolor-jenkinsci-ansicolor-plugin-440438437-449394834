FROM rshariffdeen/arja
ADD . /setup/bears/jenkinsci-ansicolor/jenkinsci-ansicolor-plugin-440438437-449394834/
RUN  bash -c 'groupadd -g 1011 cerberus' 
RUN  bash -c "cut -d: -f1 /etc/passwd | xargs -i usermod -g cerberus {} "
COPY --from=bears-jenkinsci-ansicolor-jenkinsci-ansicolor-plugin-440438437-449394834  /experiment /experiment
COPY --from=bears-jenkinsci-ansicolor-jenkinsci-ansicolor-plugin-440438437-449394834  /logs /logs
COPY --from=bears-jenkinsci-ansicolor-jenkinsci-ansicolor-plugin-440438437-449394834  /root/ /root/
RUN mvn clean compile test -Drat.skip=true -f /experiment/bears/jenkinsci-ansicolor/jenkinsci-ansicolor-plugin-440438437-449394834//src/./pom.xml; return 0
RUN  bash /setup/bears/jenkinsci-ansicolor/jenkinsci-ansicolor-plugin-440438437-449394834/deps.sh 
RUN  bash /setup/bears/jenkinsci-ansicolor/jenkinsci-ansicolor-plugin-440438437-449394834/install_deps 
ENTRYPOINT ["/bin/sh"]
